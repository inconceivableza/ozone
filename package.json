{
  "name": "ozone",
  "private": true,
  "version": "0.0.0",
  "description": "Moderation admin interface for atproto services",
  "license": "MIT or Apache 2.0",
  "engines": {
    "node": "20.x"
  },
  "packageManager": "yarn@4.8.1",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "atproto:build && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "cypress": "cypress open",
    "e2e:run": "$(yarn bin)/cypress run --browser chrome",
    "atproto:install": "cd submodules/atproto && pnpm  -F api... -F oauth-client-browser... -F oauth-types... -F xrpc... install --frozen-lockfile",
    "atproto:build": "cd submodules/atproto && pnpm  -F api... -F oauth-client-browser... -F oauth-types... -F xrpc... build"
  },
  "dependencies": {
    "@atproto/api": "link:./submodules/atproto/packages/api",
    "@atproto/oauth-client-browser": "link:./submodules/atproto/packages/oauth/oauth-client-browser",
    "@atproto/oauth-types": "link:./submodules/atproto/packages/oauth/oauth-types",
    "@atproto/xrpc": "link:./submodules/atproto/packages/xrpc",
    "@headlessui/react": "^2.2.1",
    "@heroicons/react": "^2.0.13",
    "@tanstack/react-query": "^4.22.0",
    "@types/node": "18.11.17",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@uiw/react-md-editor": "^4.0.5",
    "bcp-47-match": "^2.0.3",
    "date-fns": "^2.29.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.2.4",
    "hls.js": "^1.5.13",
    "kbar": "^0.1.0-beta.45",
    "lande": "^1.0.10",
    "next": "15.2.6",
    "next-runtime-env": "^3.3.0",
    "react": "19.1.2",
    "react-dom": "19.1.2",
    "react-dropzone": "^14.3.5",
    "react-json-view": "1.21.3",
    "react-tailwindcss-select": "^1.8.5",
    "react-tetris": "^0.3.0",
    "react-toastify": "^11.0.5",
    "react-use": "^17.4.0",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "typescript": "^5.4.5",
    "yet-another-react-lightbox": "^3.20.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "autoprefixer": "^10.4.13",
    "cypress": "^14.3.0",
    "eslint-plugin-cypress": "^4.2.1",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4"
  },
  "resolutions": {
    "jackspeak": "2.1.1",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2"
  },
  "volta": {
    "node": "20.9.0"
  }
}
